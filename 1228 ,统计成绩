 #include<stdio.h>
int main()
{
	int a;
	int A = 0, B = 0, C = 0, D = 0;
	scanf("%d\n", &a);
	int i = 0;
	for (i = 0; i < a; i++)
	{
		int b = 0;
		scanf("%d", &b);
		if (b < 60)
			D++;
		else if (b >= 60 && b < 80)
			C++;
		else if (b >= 80 && b < 90)
			B++;
		else 
			A++;
	}
	printf("%d\n%d\n%d\n%d", A, B, C, D);
	return 0;
}

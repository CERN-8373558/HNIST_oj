#include<stdio.h>

int main()
{
	int arr[100][100];
	int n, m, q;
	int i, j;
	scanf("%d%d", &n, &m);
	for (i = 0; i < n; i++)
	{
		for (j = 0; j < m; j++)
		{
			scanf("%d" ,&arr[i][j]);
		}
	}
	scanf("%d", &q);
	int k = 0;
	for (k = 0; k < q; k++)
	{
		scanf("%d%d", &i, &j);
		printf("%d\n", arr[i - 1][j - 1]);
	}
	return 0;
}

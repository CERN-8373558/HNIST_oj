#include <stdio.h>

int main()
{
    int n,i,j,s=0;
    int a[11];
    for(int i=0; i<11; i++)
    {
        scanf("%d", &a[i]);
    }
    scanf("%d", &n);
    for(i=0;i<11;i++){
        s=0;
        for(j=0;j<11;j++){
        if(i!=j){
            s+=a[j];
        }
    }
    if(s==n){
        printf("%d", a[i]);
    }
    }
    
    return 0;
}

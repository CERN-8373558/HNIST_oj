#include <stdio.h>
int main()
{
  int arr[100][100]={0};
  int n,m,q;
  scanf("%d%d%d",&n,&m,&q);
  int i,j;
  int k=0;
  for(k=0;k<q;k++)
  {
    scanf("%d%d",&i,&j);
    arr[i-1][j-1]=1;
  }
  for(i=0;i<n;i++){
    for(j=0;j<m;j++){
      printf("%d ",arr[i][j]);
    }printf("\n");
  }
  return 0;
}

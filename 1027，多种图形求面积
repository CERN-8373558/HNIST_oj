#include<stdio.h>
#define PI 3.1415926
int main()
{
  int type;
  scanf("%d",&type);
  int a,b;int S;int r;
  switch(type)
  {
    case 1:scanf("%d%d",&a,&b);S=a*b;printf("%d",S);break;
    case 2:scanf("%d%d",&a,&b);S=a*b/2;printf("%d",S);break;
    case 3:scanf("%d",&r);S=PI*r*r;printf("%d",S);break;
    default:printf("0");
  }
  return 0;
}

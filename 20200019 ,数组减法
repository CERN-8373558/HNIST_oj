#include <stdio.h>
int main()
{
  int n;
  scanf("%d",&n);
  int arr1[100]={0};
  int arr2[100]={0};
  int i=0;
  int j=0;
  for(i=0;i<n;i++)
  {
    scanf("%d",&arr1[i]);
  }
  for(i=0;i<n;i++)
  {
    scanf("%d",&arr2[i]);
  }
  int arr3[100]={0};
  for(i=0;i<n;i++)
  {
    arr3[i]=arr1[i]-arr2[i];
  }
  for(i=0;i<n;i++)
  {
    printf("%d ",arr3[i]);
  }
  return 0;
}

#include <stdio.h>
int main()
{
  int n,x,y;
  scanf("%d%d%d",&n,&x,&y);
  float a,b,c;
  a=y/x;
  b=n-a;
  c=y%x;
  if(c==0){
  printf("%.0f",b);
    }
  else{
    printf("%.0f",b-1);
  }
  return 0;
}
//无言以对啊，oj
#include<stdio.h>
int main()
{
	int n = 0;
	int x = 0;
	int y = 0;
	int num = 0;
	scanf("%d%d%d", &n, &x, &y);
	num = (int)(n - (float)y / x);
	if (num < 0)
		printf("%d", 0);
	else
		printf("%d", num);


}
